parameters:
    level: 6
    paths:
        - src
        - tests
        - docs
    reportUnmatchedIgnoredErrors: true
    treatPhpDocTypesAsCertain: true
    checkImplicitMixed: true
    tmpDir: var/phpstan
    ignoreErrors:
        - message: '#.*#'
          paths:
              - tests/Checkout/CheckoutOperationTest.php
              - tests/Charge/ChargeOperation/**.php
              - tests/Merchant/MerchantOperationTest.php

